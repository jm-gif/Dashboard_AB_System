<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Dashboard_AB_System.ViewModel"
                    xmlns:view="clr-namespace:Dashboard_AB_System.View">
    
    
    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PostVM}">
        <view:Post/>
    </DataTemplate>   

    <DataTemplate DataType="{x:Type vm:About_UsVM}">
        <view:About_Us />
    </DataTemplate>

   

</ResourceDictionary>
